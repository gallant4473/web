<h1>Listing apps</h1>
<table>
  <thead>
    <tr>
    <th></th>
      <th>Title</th>
      <th>Blog link</th>
      <th>Job link</th>
      <th>Description</th>
      <th>Author</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @apps.each do |app| %>
    <tr>
    <td>
      <div class="btn-group">
        <%= link_to like_app_path(app), method: :put, class: "btn btn-default btn-sm" do %>
        <span class="glyphicon glyphicon-chevron-up"></span>
        Upvote
        <%= app.get_upvotes.size %>
        <% end %>
        <%= link_to dislike_app_path(app), method: :put, class: "btn btn-default btn-sm" do %>
        <span class="glyphicon glyphicon-chevron-down"></span>
        Downvote
        <%= app.get_downvotes.size %>
        <% end %>
      </div></td>
      <td><%= app.title %></td>
      <td><%= app.blog_link %></td>
      <td><%= app.job_link %></td>
      <td><%= app.description %></td>
      <td><%= app.author %></td>
      <td><%= app.status %></td>
      <td><%= link_to 'Show', app %></td>
      <% if app.user == current_user %>
      <td><%= link_to 'Edit', edit_app_path(app) %></td>
      <td><%= link_to 'Destroy', app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>
<br>
